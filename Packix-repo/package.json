{
  "name": "packix_backend",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node server/server.js",
    "posttest": "npm run lint && nsp check",
    "build:sdk:admin": "./node_modules/@mean-expert/loopback-sdk-builder/bin/lb-sdk ./server/server.js ./admin/src/app/shared/sdk --wipe enabled",
    "build:sdk:client": "./node_modules/@mean-expert/loopback-sdk-builder/bin/lb-sdk ./server/server.js ./client/shared/sdk -l react -i disabled -t false --wipe enabled",
    "build:admin": "cd ./admin && npm run deploy cd ../",
    "build:client": "cd ./client && npm run build && cd ../",
    "build": "npm run build:sdk:admin && npm run build:sdk:client && cd admin && npm run deploy-prod && cd ../client && npm run build && cd ../",
    "production:start": "pm2 start ecosystem.config.js --env production",
    "production:stop": "pm2 stop ecosystem.config.js",
    "production:restart": "pm2 restart ecosystem.config.js --update-env",
    "docker": "docker build -t packix_backend . --build-arg is_building=1"
  },
  "dependencies": {
    "@mean-expert/loopback-sdk-builder": "git://github.com/ioscreatix/loopback-sdk-builder.git#development",
