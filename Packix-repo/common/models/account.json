    },
    "packagePurchases": {
      "type": "hasMany",
      "model": "PackagePurchase",
      "foreignKey": ""
    },
    "packageGifts": {
      "type": "hasMany",
      "model": "PackageGiftLink",
      "foreignKey": "accountId",
      "allowedMethods": [
        "get",
        "count"
      ]
    },
    "devices": {
      "type": "hasMany",
      "model": "Device",
      "foreignKey": "",
      "allowedMethods": [
        "get"
      ]
    },
    "packages": {
      "type": "hasMany",
      "model": "Package",
      "foreignKey": "",
      "allowedMethods": [
        "get",
        "delete",
        "destroyById"
      ]
    },
    "groups": {
      "type": "hasMany",
      "model": "AccountGroup",
      "foreignKey": "accountId",
      "through": "AccountGroupLink",
      "allowedMethods": [
        "get"
      ]
    }
  },
