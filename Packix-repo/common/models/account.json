    },
    "profileEmail": {
      "type": "string"
    },
    "cydiaId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccountToken",
      "foreignKey": "userId",
      "allowedMethods": [
        "get"
      ]
    },
    "identities": {
