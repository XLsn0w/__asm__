      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dynamicOwner",
      "permission": "ALLOW",
      "property": "prototype.patchAttributes"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dynamicOwner",
      "permission": "ALLOW",
      "property": "deleteById"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
