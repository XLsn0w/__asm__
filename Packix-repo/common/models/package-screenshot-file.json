{
  "name": "PackageScreenshotFile",
  "plural": "PackageScreenshotFiles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "silenceWarnings": false
    },
    "Archive": {
      "deletedAt": "deletedAt",
      "isDeleted": "isDeleted"
    }
  },
  "allowedMethods": [
    "find",
    "findOne",
    "findById"
  ],
  "properties": {
    "width": {
      "type": "number",
      "required": true
