    "height": {
      "type": "number",
      "required": true
    },
    "sizeKey": {
      "type": "string",
      "required": true,
      "index": true
    },
    "fileId": {
      "type": "string",
      "required": true,
      "index": true
    },
    "screenshotId": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
