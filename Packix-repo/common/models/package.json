{
  "name": "Package",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "silenceWarnings": false
    },
    "Archive": {
      "deletedAt": "deletedAt",
      "isDeleted": "isDeleted"
    }
  },
  "scope": {
    "include": [
      "latestVersion",
      "developer",
      "section"
    ]
  },
  "ownerIdPath": "latestVerion.package.accountId",
  "allowedMethods": [
    "find",
    "findOne",
    "findById",
    "destroyById",
    "prototype.patchAttributes",
    "updateAttributes",
    "count",
    "prototype.giftToEmails",
    "writeReview",
    "prototype.uploadScreenshot",
    "upload",
    "prototype.purchase",
    "prototype.generateCoupon",
    "developerPackages",
    "prototype.purchasePayPal",
    "purchasePayPal",
    "prototype.purchaseStats",
    "prototype.giftToPatreons"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "identifier": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string",
      "required": true,
      "default": "An Awesome Mobile Substrate Tweak"
    },
    "minIOSVersion": {
      "type": "string"
    },
    "maxIOSVersion": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "maintainer": {
      "type": "string"
    },
    "accountId": {
      "type": "string",
      "index": true
    },
    "visible": {
      "type": "boolean",
      "default": false
    },
    "stage": {
      "type": "string",
      "default": "stable"
    },
    "detailedDescription": {
      "type": "object"
    },
    "bugsReportURL": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "isPaid": {
      "type": "boolean",
      "default": false
    },
    "patreonRestricted": {
      "type": "boolean",
      "default": false
    },
    "hasRestrictions": {
      "type": "boolean",
      "default": false
    },
    "latestVersionId": {
      "type": "string"
    },
    "screenshots": {
      "type": [
        "PackageScreenshot"
      ],
      "default": []
    },
    "screenshotIds": {
      "type": [
        "string"
      ]
    },
    "recentReviews": {
      "type": [
        "PackageVersionReview"
      ],
      "default": []
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId",
      "options": {
        "disableInclude": true
      },
      "allowedMethods": [
        "get"
      ]
    },
    "versions": {
      "type": "hasMany",
      "model": "PackageVersion",
      "foreignKey": "",
      "allowedMethods": [
        "get",
        "count",
        "destroyById",
        "findById",
        "delete",
        "updateById"
      ]
    },
    "section": {
      "type": "belongsTo",
      "model": "Section",
      "foreignKey": "sectionId",
      "allowedMethods": [
        "get"
      ]
    },
    "purchases": {
      "type": "hasMany",
      "model": "PackagePurchase",
      "foreignKey": "",
      "options": {
        "disableInclude": true
      },
      "allowedMethods": []
    },
    "latestVersion": {
      "type": "belongsTo",
      "model": "PackageVersion",
      "foreignKey": "latestVersionId",
      "allowedMethods": [
        "get"
      ]
    },
    "downloadRestrictions": {
      "type": "hasMany",
      "model": "PackageDownloadRestriction",
      "foreignKey": "",
      "allowedMethods": [
        "get",
        "findById",
        "count",
        "updateById",
        "destroyById"
      ]
    },
    "reviews": {
      "type": "hasMany",
      "model": "PackageVersionReview",
      "foreignKey": "",
      "allowedMethods": [
        "count",
        "get",
        "findById"
      ]
    },
    "couponCodes": {
      "type": "hasMany",
      "model": "PackageCouponCode",
      "foreignKey": "",
      "options": {
        "disableInclude": true
      },
      "allowedMethods": [
        "get",
        "findById"
      ]
    },
    "developer": {
      "type": "hasOne",
      "model": "DeveloperInfo",
      "foreignKey": "accountId",
      "primaryKey": "accountId",
      "allowedMethods": [
        "get"
      ]
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "prototype.giftToEmails"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "generateCoupon",
        "purchaseStats",
        "giftToPatreons"
      ]
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "purchaseStats",
        "generateCoupon",
        "giftToPatreons"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": [
        "purchaseStats"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "writeReview"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "__create__versions",
        "__delete__versions",
        "__destroyById__versions",
        "__updateById__versions"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "__create__downloadRestrictions",
        "__delete__downloadRestrictions",
        "__destroyById__downloadRestrictions",
        "__updateById__downloadRestrictions"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "__count__purchases",
        "__create__purchases",
        "__delete__purchases",
        "__destroyById__purchases",
        "__findById__purchases",
        "__get__purchases",
        "__updateById__purchases"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "__create__reviews",
        "__delete__reviews",
        "__destroyById__reviews",
        "__updateById__reviews"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": [
        "__count__couponCodes",
        "__create__couponCodes",
        "__delete__couponCodes",
        "__destroyById__couponCodes",
        "__findById__couponCodes",
        "__get__couponCodes",
        "__updateById__couponCodes"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dynamicOwner",
      "permission": "ALLOW",
      "property": [
        "__create__reviews",
        "__delete__reviews",
        "__destroyById__reviews",
        "__updateById__reviews",
        "__get__reviews"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dynamicOwner",
      "permission": "ALLOW",
      "property": [
        "__count__couponCodes",
        "__create__couponCodes",
        "__delete__couponCodes",
        "__destroyById__couponCodes",
        "__findById__couponCodes",
        "__get__couponCodes",
        "__updateById__couponCodes"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dynamicOwner",
      "permission": "ALLOW",
      "property": [
        "__count__purchases",
        "__delete__purchases",
        "__destroyById__purchases",
        "__findById__purchases",
        "__get__purchases",
        "__updateById__purchases"
      ]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "developer",
      "permission": "ALLOW",
      "property": [
        "upload",
        "developerPackages"
      ]
    }
  ],
  "methods": {}
}
