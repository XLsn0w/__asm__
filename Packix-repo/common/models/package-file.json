{
  "name": "PackageFile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "ownerIdPath": "packageVersion.package.accountId",
  "allowedMethods": [
    "findById"
  ],
  "mixins": {
    "Archive": {
      "deletedAt": "deletedAt",
      "isDeleted": "isDeleted"
    }
