      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "string",
      "required": false
    },
    "md5": {
      "type": "string",
      "required": true
    },
    "sha1": {
      "type": "string",
      "required": true
    },
    "sha256": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "container": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "string",
      "required": false
    },
    "packageVersionId": {
      "type": "string",
      "index": true
    },
    "fileDownloadId": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
