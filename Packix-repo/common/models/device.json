{
  "name": "Device",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "deviceModelName": "getModelName"
      }
    },
    "Archive": {
      "deletedAt": "deletedAt",
      "isDeleted": "isDeleted"
    }
  },
  "properties": {
    "udid": {
      "type": "string",
      "required": true,
      "index": true
    },
    "deviceVersion": {
      "type": "string",
      "index": true,
      "default": "Unknown"
    },
    "deviceModel": {
      "type": "string",
      "required": true,
      "index": true
    },
    "deviceModelName": {
      "type": "string"
    },
    "accountId": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": ""
    },
    "downloads": {
      "type": "hasMany",
      "model": "PackageDownload",
      "foreignKey": "clientUDID",
      "primaryKey": "udid"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "developer",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
