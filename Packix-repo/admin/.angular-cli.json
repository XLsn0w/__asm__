        "../node_modules/angular2-toaster/src/toaster.scss",
        "assets/styles/custom.scss"
      ],
      "stylePreprocessorOptions": {
        "includePaths": [
           "assets/styles",
           "assets/styles/bootstrap",
           "assets/styles/mixins",
           "assets/styles/skins",
           "../../client/static/assets"
        ]
      },
      "scripts": [
        "assets/vendor/jquery/jquery.js",
        "assets/vendor/tether/tether.js",
        "assets/vendor/bootstrap/bootstrap.js",
        "assets/vendor/animsition/animsition.js",
        "assets/vendor/asscrollable/jquery-asScrollable.js",
        "assets/vendor/asscrollbar/jquery-asScrollbar.js",
        "assets/vendor/ashoverscroll/jquery-asHoverScroll.js",
        "assets/vendor/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js",
        "assets/vendor/bootstrap-select/bootstrap-select.js",
        "assets/vendor/breakpoints/breakpoints.js",
        "../node_modules/css-element-queries/src/ResizeSensor.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "serve": {
      "port": 8082,
      "host": "127.0.0.1"
    }
  }
}
